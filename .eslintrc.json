{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "env": {
    "jest": true
  },
  "globals": {
    "_": false,
    "React": false,
    "ReactDOM": false,
    "TestUtils": false
  },
  "rules": {
    "arrow-body-style": 1,
    "comma-dangle": 1,
    "max-len": [1, 150],
    "no-else-return": 0,
    "prefer-const": 1,
    "prefer-template": 1,
    "react/react-in-jsx-scope": 0,
    "react/prop-types": 1,
    "key-spacing": 0,
    "no-use-before-define": 0
  },
  "ecmaFeatures": {
    "experimentalObjectRestSpread": true
  }
}
